<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "bulletml.dtd">
<bulletml type="vertical" xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml%22%3E">
  <action label="top">
    <repeat>
    
      <times>2</times>
      
      <action>
          <actionRef label = "split">
          <param>15</param>
          </actionRef>
        <wait>10</wait>
      </action>
      
    </repeat>





    <action label = "split">
        <fire>
          <direction type="aim">-$1</direction>
          <speed type="absolute">0.25</speed>
          <bullet label="Default" />
        </fire>
        <fire>
          <direction type="aim">$1</direction>
          <speed type="absolute">0.25</speed>
          <bullet label="Default" />
        </fire>


    </action>









  </action>










</bulletml>